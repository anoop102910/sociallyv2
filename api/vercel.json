{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "dist/**/*",
            "node_modules/**/*"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  